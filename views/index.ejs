<%- include layouts/icon.ejs %>

<%- include layouts/header.ejs %>
<h1>Welcome To Twitter!</h1>
<a href="/login">Login</a>
<a href="/user/register">Signup</a>

<% if (req.isAuthenticated()) { %>
  <% if (req.user !== null) { %>
    <br>
    <%= req.user.username %>
    <br>
    <%= req.user.password %>
  <% } %>
<% } %>
