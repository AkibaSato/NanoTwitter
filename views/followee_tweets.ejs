<%- include layouts/icon.ejs %>

<%- include layouts/header.ejs %>
<h1><%= user.fullName %>'s Followee Tweetsa</h1>
<div class="tweet-list">
  <% for(var i = 0; i < tweets.length; i++) { %>
    <div class="tweets">
      <div class="row">
        <h2><%= tweets[i].user.fname %> <%= tweets[i].user.lname %></h2>
        <h3><span><a href="/user/<%= tweets[i].user.id %>"> @<%= tweets[i].user.username %></a></span> </h3>
      </div>
      <div class="row">
        <h4><%= tweets[i].content %></h4>
      </div>
      <br>
      <div class="tweet-border"></div>
      <br>
    </div>
  <% } %>
</div>
