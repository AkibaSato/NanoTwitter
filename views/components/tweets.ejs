

<div class="tweet-list">
  <% for(var i=0; i<tweets.length; i++) { %>
    <div class="tweets">
      <div class="row">
        <h2><%= tweets[i].user.fname %> <%= tweets[i].user.lname %></h2>

        <h3>@<%= tweets[i].user.username %> â€¢ <%= tweets[i].createdAt.getDate() %> <%= tweets[i].createdAt.getMonth() %> <%= tweets[i].createdAt.getFullYear() %></h3>
      </div>
      <div class="row">
        <h4><%= tweets[i].content %></h4>
      </div>
    </div>
    <div class="tweet-border">____________________________________________________________________</div>
  <% } %>
</div>
